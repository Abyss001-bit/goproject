<!--Admins/daoru/daoru.wxml-->
<view class="all">
  <text>删除单个座位</text>
  <view class="contain">
    <view>
    <text>馆号信息</text><input type="text" value="{{fenguan}}" placeholder="图书馆名" name="fenguan" bindinput="fenguanInput"/>
    </view>

    <view>
    <text>楼层信息</text><input type="text" value="{{louceng}}" placeholder="楼层" name="louceng" bindinput="loucengInput"/>
    </view>

    <view>
    <text>室号信息</text><input type="text" value="{{bianhao}}" placeholder="室号" name="bianhao" bindinput="bianhaoInput"/>
    </view>

    <view>
    <text>编号信息</text><input type="number" value="{{number}}" placeholder="座位号" name="number" bindinput="numberInput"/>
    </view>
    <button class="buttom" bindtap="submit">确认</button>
  </view>
</view>